  "scripts": {
    "format": "prettier --write 'src/**/*.{ts,tsx,js,json,jsx}' && prettier --write '../cloud/**/*.{js,json}'",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "build": "yarn format && BUILD_PATH=dist node scripts/build-dev.js",
    "build:prod": "yarn format && yarn install && node scripts/build.js",
    "start": "serve -p 8080 ./dist -s",
    "start:prod": "http-server -p 8080 ./build",
    "iterate": "yarn build && yarn start",
    "deploy": "yarn build && rm -rf ../public/* && cp -r ./dist/* ../public && b4a deploy ShadowboxxDevTest",
    "deploy:prod": "yarn build:prod && rm -rf ../public/* && cp -r ./build/* ../public && b4a deploy Shadowboxx"
