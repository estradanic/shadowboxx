    "@tanstack/react-query-devtools": "^4.3.5",
    "base64-js": "^1.5.1",
    "browser-image-resizer": "^2.4.1",
    "browserslist": "^4",
    "buffer": "^6.0.3",
    "classnames": "^2.3.2",
    "events": "^3.3.0",
    "luxon": "^3.3.0",
    "notistack": "^1.0.5",
    "p-queue": "^7.3.4",
    "parse": "3.4.4",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-reverse-portal": "^2.1.0",
    "react-router-dom": "6.14.2",
