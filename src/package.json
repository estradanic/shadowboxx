{
  "name": "shadowboxx",
  "version": "0.3.38",
  "license": "GPL3",
  "private": true,
  "type": "module",
  "engines": {
    "node": "16.x"
  },
  "resolutions": {
    "glob-parent": "^6.0.1"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@parse/react": "^0.0.1-alpha.18",
    "@tanstack/react-query": "^4.3.4",
    "@tanstack/react-query-devtools": "^4.3.5",
    "browser-image-resizer": "^2.4.1",
    "browserslist": "^4",
    "classnames": "^2.3.2",
    "events": "^3.3.0",
    "notistack": "^1.0.5",
    "parse": "^3.4.4",
    "react": "^17.0.1",
    "react-detect-offline": "^2.4.5",
    "react-dom": "^17.0.1",
    "react-reverse-portal": "^2.1.0",
    "react-router-dom": "^6.3.0",
    "use-debounce": "^8.0.4",
    "uuid": "^9.0.0"
  },
  "scripts": {
    "preBuild": "cd service-worker && tsc && cd ..",
    "build": "yarn preBuild && vite build --mode development",
    "build:prod": "yarn preBuild && vite build",
    "start": "yarn preBuild && vite serve",
    "start:prod": "yarn build:prod && vite preview",
    "compileCloud": "cd ./cloud && tsc && cd ..",
    "format": "prettier --write '{@types,app,cloud,components,contexts,hooks,resources,types,utils,views,service-worker}/**/*.{ts,tsx,js,json,jsx}' && prettier --write '*.{ts,tsx,js,json,jsx}'",
    "analyze": "source-map-explorer 'build/assets/*.js'",
    "preDeploy": "yarn install && yarn build && yarn compileCloud && rm -rf ../public/* && cp -r ./build/* ../public",
    "preDeploy:prod": "yarn install && yarn build:prod && yarn compileCloud && rm -rf ../public/* && cp -r ./build/* ../public",
    "deploy": "yarn preDeploy && b4a deploy ShadowboxxDevTest",
    "deploy:prod": "yarn preDeploy:prod && b4a deploy Shadowboxx"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/plugin-syntax-flow": "^7.18.6",
    "@babel/plugin-transform-react-jsx": "^7.19.0",
    "@canvas/image": "^1.0.1",
    "@types/express": "4.17.1",
    "@types/parse": "^2.18.18",
    "@types/react-detect-offline": "^2.4.1",
    "@types/react-dom": "^18.0.6",
    "@types/react-router-dom": "^5.3.3",
    "@types/sharp": "^0.31.0",
    "@types/uuid": "^8.3.3",
    "@vitejs/plugin-react": "^2.1.0",
    "blockhash-core": "^0.1.0",
    "eslint": "^8.24.0",
    "eslint-config-react-app": "^7.0.1",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4",
    "vite": "^3.1.6",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-html": "^3.2.0"
  }
}
