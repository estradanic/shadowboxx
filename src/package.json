  "scripts": {
    "preBuild": "cd service-worker && tsc && cd ..",
    "build": "yarn preBuild && vite build --mode development",
    "build:prod": "yarn preBuild && vite build",
    "start": "yarn preBuild && vite serve",
    "start:prod": "yarn build:prod && vite preview",
    "compileCloud": "cd ./cloud && tsc && cd ..",
    "format": "prettier --write '{@types,app,cloud,components,contexts,hooks,resources,types,utils,views,service-worker}/**/*.{ts,tsx,js,json,jsx}' && prettier --write '*.{ts,tsx,js,json,jsx}'",
    "analyze": "source-map-explorer 'build/assets/*.js'",
    "preDeploy": "yarn install && yarn build && yarn compileCloud && rm -rf ../public/* && cp -r ./build/* ../public",
    "preDeploy:prod": "yarn install && yarn build:prod && yarn compileCloud && rm -rf ../public/* && cp -r ./build/* ../public",
    "deploy": "yarn preDeploy && b4a deploy ShadowboxxDevTest",
    "deploy:prod": "yarn preDeploy:prod && b4a deploy Shadowboxx"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
