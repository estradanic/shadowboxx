{
  "compilerOptions": {
    "target": "es6",
    "lib": [
      "esnext"
    ],
    "allowJs": false,
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "outDir": "../../cloud",
    "skipLibCheck": true,
    "downlevelIteration": true
  }
}